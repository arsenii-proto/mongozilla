{
  "extends": ["plugin:prettier/recommended"],
  "root": true,
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "rules": {
    "no-console": "off",
    "no-debugger": "off"
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "sourceType": "module",
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "spread": true,
      "experimentalObjectRestSpread": true
    }
  }
}
