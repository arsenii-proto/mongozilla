{
  "presets": [
    "es2015",
    "stage-0"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "alias": {
          "@": [
            "./"
          ]
        }
      }
    ],
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}